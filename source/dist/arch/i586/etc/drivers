video
	/dev/vga 0110 output
	/dev/vesa 0110 output
zero
	/dev/zero 0666 root
random
	/dev/random 0666 root
null
	/dev/null 0666 root
pipe
	/dev/pipe 0666 root
speaker
	/dev/speaker 0110 audio
ps2
	/dev/keyb 0110 input
	/dev/mouse 0110 input
tcpip
	/dev/socket 0770 network
	/dev/tcpip 0110 network
http
	/dev/http 0444 network
uimng /dev/vga /dev/vesa
	/dev/uimng 0110 input
